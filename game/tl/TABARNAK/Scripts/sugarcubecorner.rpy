translate TABARNAK sell_bake_56c91265:

    # p "Thank you! Now, if you don't have anything else, I'll see if I can help someone else."
    p "Merci ! Maintenant, si tu n'as rien d'autre, je vais voir si je peux aider quelqu'un d'autre."

# game/Scripts/sugarcubecorner.rpy:456
translate TABARNAK sell_bake_1adebe60:

